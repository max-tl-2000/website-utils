    "@babel/preset-env": "7.4.4",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "7.0.0",
    "@babel/runtime": "7.3.1",
    "@emotion/core": "10.0.7",
    "@emotion/styled": "10.0.7",
    "@redisrupt/bnr": "5.0.2",
    "@redisrupt/eslint-red": "5.0.0",
    "@redisrupt/react-responsive-carousel": "3.2.0",
    "@redisrupt/react-svg-loader": "1.0.1",
    "@redisrupt/red-svg-icons": "1.0.5",
    "@splidejs/react-splide": "^0.4.4",
    "@storybook/addon-actions": "5.1.11",
