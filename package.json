{
  "name": "@redisrupt/website-utils",
  "version": "8.0.2-branch-22-02-07.21",
  "description": "Website utils for reva websites",
  "main": "dist/website-utils.min.js",
  "files": [
    "dist/",
    "server/assets/",
    "server/layouts/",
    "server/parts/",
    "server/views/",
    "storybook/resources/assets/"
  ],
  "scripts": {
    "storybook": "start-storybook -p 6006 -c ./storybook -s ./storybook/resources/",
    "build-storybook": "build-storybook -c ./storybook -o ./dist/storybook",
    "lint": "eslint --cache --cache-location node_modules/.cache/ -f friendly 'src/**/*.js' 'storybook/**/*.js' 'resources/**/*.js' '*.js'",
    "autofix": "npm run styles:fix && npm run lint -- --fix",
    "styles:check": "npm run styles:fix -- -k",
    "styles:fix": "cssbrush './src/**/*.scss' '!./src/**/*_common.scss' './storybook/**/*.scss'",
    "prepublishOnly": "npm run build",
    "build": "npm run lint && npm run styles:check && rm -rf ./predist && rm -rf ./dist && bnr build --production && bnr buildLegacy --production && mkdir -p ./dist/ && npm run min",
    "test": "TZ=UTC jest",
    "types:check": "tsc --noEmit --skipLibCheck",
    "min": "npm run min:js && npm run min:js:legacy && npm run min:css && npm run min:css:legacy && cp -rv ./predist/maps-deps ./dist/maps-deps",
    "min:js": "uglifyjs ./predist/website-utils.js -o ./dist/website-utils.min.js --comments=/^!/",
    "min:css": "csso ./predist/website-utils.css -o ./dist/website-utils.min.css --comments exclamation && ./bnr min-pages",
    "min:js:legacy": "uglifyjs ./predist/website-utils-legacy.js -o ./dist/website-utils-legacy.min.js --comments=/^!/",
    "min:css:legacy": "csso ./predist/website-utils-legacy.css -o ./dist/website-utils-legacy.min.css --comments exclamation && ./bnr min-pages",
    "start:dev": "sx './bnr build --watch' 'npm run start:server' './bnr serve'",
    "start:dev:legacy": "sx './bnr buildLegacy --watch' './bnr start:server'",
    "start:prod": "./bnr start:server --production",
    "start:server": "nodemon --exec babel-node ./server/app.js --watch ./config.js --watch ./server/app.js",
    "check": "npm run lint && npm run styles:check && npm run types:check && npm run test"
  },
  "author": "Reva INC.",
  "license": "UNLICENSED",
  "bnrConfig": "./tasks.js",
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.3",
    "@babel/node": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.3.3",
    "@babel/plugin-proposal-decorators": "7.3.0",
    "@babel/plugin-proposal-do-expressions": "7.2.0",
    "@babel/plugin-proposal-export-default-from": "7.2.0",
