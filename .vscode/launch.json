  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "server",
      "program": "${workspaceFolder}/}"
    },
    {
      "type": "node",
      "request": "launch",
      "sourceMaps": true,
      "name": "debug-current-file",
      "showAsyncStacks": true,
      "runtimeArgs": [
        "--require",
        "./babel-register.js",
        "--inspect-brk"
